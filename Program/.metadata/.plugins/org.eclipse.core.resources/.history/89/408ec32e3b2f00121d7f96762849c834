package pl.wroc.pwr.student.acteditor.view;

import org.eclipse.swt.widgets.Display;

public class WindowBuilder {
	private Display display;
	
	public WindowBuilder(Display display) {
		this.display = display;
	}
	
	public void openWindow(int choice) {
		SubWindow window;
		switch(choice) {
		case 0:
			window = new SubWindow(display, "ustawa");
			break;
		case 1:
			window = new SubWindow(display, "rozporzadzenie");
			break;
		case 2:
			window = new SubWindow(display, "uchwala");
			break;
		}
	}
}
