package pl.wroc.pwr.student.acteditor;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;

public class Schema {
	public Schema() {
		String content = "";
		try {
			BufferedReader in = new BufferedReader(new FileReader("D:\\Opera Unite\\EDAP\\Program\\ActEditor\\resources\\schema.xsd"));
			String str = "";
			while ((str = in.readLine()) != null) {
				content += "\"" + str.replaceAll("\"", "\\\\\"") + "\"+\n";
			}
			in.close();
		} catch (IOException e) {
		}
		System.out.println(content);
	}
}
