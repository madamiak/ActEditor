		<tag html="sub" xml="indeks-dolny" ommit="true"/>
        <tag html="table" xml="tabela"/>
		<!-- tagi specjalnego znaczenia, sa one pomijane 
		      ale powoduja przestawienie flagi, od nich zalezec bedzie czy w jaki sposob zostanie zwizualizowany element tr lub th z ich srodka
		-->
		<tag html="thead" xml="naglowek-tabeli" />
		<tag html="tbody" xml="tbody" ommit="true"/>
		<tag html="tfoot" xml="stopka-tabeli"/>        
        <tag html="tr" xml="wiersz"/>		
        <tag html="th" xml="th-komorka"/>
		<tag html="td" xml="komorka"> <!-- bedzie korygowana na th -->
            <attribute name="style" value="text-align: left;" xml-name="wyrownanie" xml-value="do-lewej" fop-name="text-align" fop-value="left"/>
            <attribute name="style" value="text-align: center;" xml-name="wyrownanie" xml-value="wysrodkowane" fop-name="text-align" fop-value="center"/>
            <attribute name="style" value="text-align: right;" xml-name="wyrownanie" xml-value="do-prawej" fop-name="text-align" fop-value="right"/>
            <attribute name="style" value="text-align: justify;" xml-name="wyrownanie" xml-value="wyjustowane" fop-name="text-align" fop-value="justify"/>

            <attribute name="align" value="left" xml-name="wyrownanie" xml-value="do-lewej" reverse="true" fop-name="text-align" fop-value="left"/>
            <attribute name="align" value="center" xml-name="wyrownanie" xml-value="wysrodkowane" reverse="true" fop-name="text-align" fop-value="center"/>
            <attribute name="align" value="right" xml-name="wyrownanie" xml-value="do-prawej" reverse="true" fop-name="text-align" fop-value="right"/>
            <attribute name="align" value="justify" xml-name="wyrownanie" xml-value="wyjustowane" reverse="true" fop-name="text-align" fop-value="justify"/>

            <attribute name="style" value="vertical-align: top;" xml-name="wyrownanie-w-pionie" xml-value="do-gory" fop-name="vertical-align" fop-value="top"/>
            <attribute name="style" value="vertical-align: bottom;" xml-name="wyrownanie-w-pionie" xml-value="do-dolu" fop-name="vertical-align" fop-value="bottom"/>
            <attribute name="style" value="vertical-align: center;" xml-name="wyrownanie-w-pionie" xml-value="wysrodkowane" fop-name="vertical-align" fop-value="center"/>
			
            <attribute name="valign" value="top" xml-name="wyrownanie-w-pionie" xml-value="do-gory" reverse="true" fop-name="vertical-align" fop-value="top"/>
            <attribute name="valign" value="bottom" xml-name="wyrownanie-w-pionie" xml-value="do-dolu" reverse="true" fop-name="vertical-align" fop-value="bottom"/>
            <attribute name="valign" value="center" xml-name="wyrownanie-w-pionie" xml-value="wysrodkowane" reverse="true" fop-name="vertical-align" fop-value="center"/>
			
            <attribute name="rowspan" xml-name="scal-pionowo" fop-name="number-rows-spanned" reverse="true"/> <!-- jesli nie mam value - to znaczy ze wartosz bedzie przepisana -->
            <attribute name="colspan" xml-name="scal-poziomo" fop-name="number-columns-spanned" reverse="true"/>
        </tag>                
        
